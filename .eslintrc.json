{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": [
            "error",
            {
              "code": 128
            }
        ],
        "require-jsdoc": "off",
        "arrow-parens": ["error", "as-needed"]

    }
}
